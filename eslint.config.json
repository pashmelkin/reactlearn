{
  "ecmaFeatures": {
    "jsx": true
  },
    "env": {
        "browser": true,
        "node": true,
        "jquery": true,
        "es6": false
    },
    "globals": {
      jQuery: true,
      $: true
    },
    "rules": {
        "comma-dangle": 0,
        "padded-blocks": 0,
        "strict": 0,
        "no-unused-vars": 1,
        "no-multi-str": 0,
        "linebreak-style": 0
    }
}
